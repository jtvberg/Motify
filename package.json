		"globals": "^17.0.0",
		"prettier": "^3.8.0",
		"prettier-plugin-svelte": "^3.4.1",
		"svelte": "^5.46.4",
		"svelte-check": "^4.3.5",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.53.0",
		"vite": "^7.3.1"
